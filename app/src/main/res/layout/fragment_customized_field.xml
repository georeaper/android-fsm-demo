<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.ui.Configuration.MasterData.CustomizedFieldFragment">
    <ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@color/background_primary_lightB">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp">
                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="22sp"
                        android:text="Equipment"
                        android:textStyle="bold"
                        android:padding="4dp"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:layout_marginEnd="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginTop="4dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.8"
                        android:orientation="vertical"
                        >
                        <com.google.android.material.textview.MaterialTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:text="Manufacturer"
                            android:textStyle="bold"
                            android:paddingTop="8dp"
                            android:paddingBottom="2dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />
                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/settingsManufacturer_overview"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="Ventilators , Monitors "
                            android:textColor="@color/color_grey"

                            android:paddingBottom="4dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.2"
                        android:gravity="center" >

                        <ImageButton
                            android:id="@+id/masterData_manufacturer_btn"
                            android:layout_width="48dp"
                            android:layout_height="48dp"

                            android:contentDescription="TODO"
                            android:background="@android:color/transparent"
                            android:src="@drawable/navigation_list_right"/>


                    </LinearLayout>


                </LinearLayout>
                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:dividerThickness="2dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:layout_marginEnd="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginTop="4dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.8"
                        android:orientation="vertical">
                        <com.google.android.material.textview.MaterialTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:text="Model"
                            android:textStyle="bold"
                            android:paddingTop="8dp"
                            android:paddingBottom="2dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />
                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/settingsModel_overview"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="A7 ,A8 , R12 , "
                            android:textColor="@color/color_grey"

                            android:paddingBottom="4dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.2"
                        android:gravity="center" >

                        <ImageButton
                            android:id="@+id/masterData_model_btn"
                            android:layout_width="48dp"
                            android:layout_height="48dp"

                            android:contentDescription="TODO"
                            android:background="@android:color/transparent"
                            android:src="@drawable/navigation_list_right"/>


                    </LinearLayout>


                </LinearLayout>
                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:dividerThickness="2dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:layout_marginEnd="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginTop="4dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.8"
                        android:orientation="vertical">
                        <com.google.android.material.textview.MaterialTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:text="Category"
                            android:textStyle="bold"
                            android:paddingTop="8dp"
                            android:paddingBottom="2dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />
                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/settingsCategory_overview"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="Ventilators , Monitors "
                            android:textColor="@color/color_grey"

                            android:paddingBottom="4dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.2"
                        android:gravity="center" >

                        <ImageButton
                            android:id="@+id/masterData_category_btn"
                            android:layout_width="48dp"
                            android:layout_height="48dp"

                            android:contentDescription="TODO"
                            android:background="@android:color/transparent"
                            android:src="@drawable/navigation_list_right"/>


                    </LinearLayout>


                </LinearLayout>
                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:dividerThickness="4dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"/>





            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@color/background_primary_lightB">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp">
                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="22sp"
                        android:text="Work orders"
                        android:textStyle="bold"
                        android:padding="4dp"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:layout_marginEnd="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginTop="4dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.8"
                        android:orientation="vertical">
                        <com.google.android.material.textview.MaterialTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:text="Work order type"
                            android:textStyle="bold"
                            android:paddingTop="8dp"
                            android:paddingBottom="2dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />
                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/settingsWorkOrders_types"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="Service , Sales ,Installation, Repair "
                            android:textColor="@color/color_grey"

                            android:paddingBottom="4dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.2"
                        android:gravity="center" >

                        <ImageButton
                            android:id="@+id/masterData_workorderType_btn"
                            android:layout_width="48dp"
                            android:layout_height="48dp"

                            android:contentDescription="TODO"
                            android:background="@android:color/transparent"
                            android:src="@drawable/navigation_list_right"/>


                    </LinearLayout>


                </LinearLayout>
                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:dividerThickness="2dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:layout_marginEnd="2dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginTop="4dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.8"
                        android:orientation="vertical">
                        <com.google.android.material.textview.MaterialTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:text="Departments"
                            android:textStyle="bold"
                            android:paddingTop="8dp"
                            android:paddingBottom="2dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />
                        <com.google.android.material.textview.MaterialTextView
                            android:id="@+id/settingsWorkOrders_departments"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:text="Operating Theatre ,Cardiology ,"
                            android:textColor="@color/color_grey"

                            android:paddingBottom="4dp"
                            android:paddingStart="4dp"
                            android:paddingEnd="8dp"
                            />


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.2"
                        android:gravity="center" >

                        <ImageButton
                            android:id="@+id/masterData_workorderDepartment_btn"
                            android:layout_width="48dp"
                            android:layout_height="48dp"

                            android:contentDescription="TODO"
                            android:background="@android:color/transparent"
                            android:src="@drawable/navigation_list_right"/>


                    </LinearLayout>


                </LinearLayout>
                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:dividerThickness="4dp"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"/>

            </LinearLayout>

        </LinearLayout>

    </ScrollView>





</FrameLayout>